{
  "extends": "@tsconfig/node16-strictest/tsconfig.json",
  "compilerOptions": {
    "lib": ["es2021", "dom"],
    "module": "ESNext",
    "checkJs": false,
    "resolveJsonModule": true,
    "exactOptionalPropertyTypes": false,
    "typeRoots": ["./node_modules/@types"],
    "noEmit": true,
    "declaration": true
  },
  "include": ["src/**/*"]
}
